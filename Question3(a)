using System;

class ATMSystem
{
    static void Main()
    {
        int correctPIN = 1234;
        int attempts = 0;
        bool success = false;

        while (attempts < 3)
        {
            Console.Write("Enter PIN: ");
            int enteredPIN = Convert.ToInt32(Console.ReadLine());

            if (enteredPIN == correctPIN)
            {
                Console.WriteLine("Access Granted");
                success = true;
                break;
            }
            else
            {
                Console.WriteLine("Wrong PIN");
                attempts++;
            }
        }

        if (!success)
            Console.WriteLine("Card Blocked");
    }
}
