using System;

class GameScores
{
    static void Main()
    {
        int playerA = 20, playerB = 15;

        if (playerA > playerB)
            Console.WriteLine("Player A scored more than Player B");
        if (playerA == playerB)
            Console.WriteLine("Scores are equal");
        if (playerA <= playerB)
            Console.WriteLine("Player A failed to beat Player B");
        if (playerB <= playerA)
            Console.WriteLine("Player B failed to beat Player A");
    }
}
